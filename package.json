{
  "name": "senior-celebration",
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "postcss src/_includes/css/tailwind.css -o src/css/styles.css",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "postcss src/_includes/css/tailwind.css -o src/css/styles.css --watch",
    "watch:html": "eleventy --serve",
    "start": "npm run watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "eleventy-plugin-sass": "^1.0.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.2.0",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  }
}
